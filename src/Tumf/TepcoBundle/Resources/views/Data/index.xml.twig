<?xml version="1.0" encoding="UTF-8"?>
{% block body %}
<data>
  <banner>{{data.banner}}</banner>
  <capacity>{{data.capacity}}</capacity>
  <timestamp>{{data.timestamp}}</timestamp>
  <trend>
    {% for time,line in data.trend %}
    <time at="{{time}}">
      <today>{{line.today}}</today>
      <yesterday>{{line.yesterday}}</yesterday>
    </time>
    {% endfor %}
  </trend>
</data>
{% endblock %}
